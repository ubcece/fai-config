# <type> <mountpoint> <size in mb> [mount options]     [;extra options]

# Leave Mac OS X on disk1. Manually partition disk2 and install Linux there.
disk_config disk2

# EFI partition
primary  -             preserve1   ro,errors=remount-ro
primary  swap          16384       rw
primary  /             16384       rw,errors=remount-ro ; -m 5 -j ext3
primary  /data         preserve4   rw,nosuid            ; -m 1 -j hfsplus

# Preserve partitions 1 and 2 of disk1 (for Windows dual-boot)

disk_config disk1 disklabel:msdos preserve_lazy:1,2 fstabkey:uuid bootable:4 align-at:8k
primary		-		0-	-	-
primary		-		0-	-	-
primary		swap		2G	swap	rw
primary		-		38G-	-	-

disk_config lvm fstabkey:uuid preserve_lazy:vg-data
vg		vg		disk1.4
vg-root1	/		35G-60G	ext4	rw,errors=remount-ro
vg-var1		/var		6G-8G	ext4	rw
vg-root2	/altroot	35G-60G	ext4	rw,errors=remount-ro
vg-var2		/altroot/var	6G-8G	ext4	rw
vg-data		/data		5G-	xfs	rw

vg-snapreserve	-		1G	-	-

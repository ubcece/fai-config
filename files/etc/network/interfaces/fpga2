# 2009-02-19 Chris Dumont. fpga2 is a Dell PowerEdge 1950. 

auto lo eth0 eth1
iface lo inet loopback

iface eth0 inet static
        address 137.82.121.62
        netmask 255.255.255.0
        network 137.82.121.0
        broadcast 137.82.121.255
        gateway 137.82.121.254
        dns-nameservers 137.82.52.165 137.82.52.170
        dns-search ece.ubc.ca

iface eth1 inet static
	address 10.4.4.4
	network 10.0.0.0
	broadcast 10.255.255.254
	netmask 255.0.0.0

default fai-install-jessie

label localboot
    kernel chain
    append hd0

label fai-install-jessie
    kernel ifcpu64.c32
    append fai-install-jessie-amd64 -- fai-install-jessie-i386

label fai-install-jessie-amd64
    kernel fai/vmlinuz-jessie-amd64
    append ip=dhcp initrd=fai/initrd-jessie-amd64 root=nfs:/exports/fai/nfsroot-jessie-amd64 aufs nomodeset FAI_FLAGS=sshd,reboot,initial FAI_ACTION=install FAI_CLASSES=${FAI_CLASSES:=} hostname=$hostname auth=$auth FAI_CONFIG_SRC_REF=${FAI_CONFIG_SRC_REF:=}

label fai-install-jessie-i386
    kernel fai/vmlinuz-jessie-i386
    append ip=dhcp initrd=fai/initrd-jessie-i386  root=nfs:/exports/fai/nfsroot-jessie-i386  aufs nomodeset FAI_FLAGS=sshd,reboot,initial FAI_ACTION=install FAI_CLASSES=${FAI_CLASSES:=} hostname=$hostname auth=$auth FAI_CONFIG_SRC_REF=${FAI_CONFIG_SRC_REF:=}

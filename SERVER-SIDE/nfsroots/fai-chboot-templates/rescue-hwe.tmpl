default fai-rescue-buster-hwe

label localboot
    kernel chain
    append hd0

label fai-rescue-buster-hwe
    kernel ifcpu64.c32
    append fai-rescue-buster-amd64-hwe -- fai-rescue-buster-i386-hwe

label fai-rescue-buster-amd64-hwe
    kernel fai/vmlinuz-buster-amd64-hwe
    append ip=dhcp initrd=fai/initrd-buster-amd64-hwe root=nfs4:fai.ece.ubc.ca:/exports/fai/nfsroot-buster-amd64:vers=3 rootovl nomodeset net.ifnames=0 FAI_FLAGS=sshd,createvt FAI_CLASSES=${FAI_CLASSES:=} hostname=$hostname auth=$auth FAI_CONFIG_SRC_REF=${FAI_CONFIG_SRC_REF:=}

label fai-rescue-buster-i386-hwe
    kernel fai/vmlinuz-buster-i386-hwe
    append ip=dhcp initrd=fai/initrd-buster-i386-hwe  root=nfs4:fai.ece.ubc.ca:/exports/fai/nfsroot-buster-i386  rootovl nomodeset net.ifnames=0 FAI_FLAGS=sshd,createvt FAI_CLASSES=${FAI_CLASSES:=} hostname=$hostname auth=$auth FAI_CONFIG_SRC_REF=${FAI_CONFIG_SRC_REF:=}

#! /bin/bash
# Chris Dumont 2007-08-10. Do only what is necessary for cfengine to run for
# the first time.

ifclass SQUEEZE && fcopy -BM /etc/ntp.conf
fcopy -BM /etc/ferm/ferm.conf
fcopy -BM /etc/default/nfs-common
fcopy -iBM /etc/default/ntp
fcopy -BM /etc/fai/fai.conf
fcopy -BM -r /etc/cfengine/

